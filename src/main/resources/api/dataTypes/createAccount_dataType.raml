#%RAML 1.0 DataType
type: object
properties:
  accountNum:
    type: string
    required: true
    maxLength: 10
    minLength: 10
  atmPin:
    type: number
    required: true
    maximum: 9999
    minimum: 1000

  ifscCode:
    required: true
    minLength: 4
    maxLength: 10
  depositAmount:
    type: number
    required: false
  mailId:
    required: true
    type: string
  contact:
    required: true
    type: string
